			<version>1.1.1</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.2.1</version>
		</dependency>

		<dependency>
			<groupId>snapdeal.suggester.client</groupId>
			<artifactId>SnapdealSuggesterClient</artifactId>
			<version>5.6-SNAPSHOT</version>
		</dependency>

		<dependency>
