			<artifactId>SDPointsClient</artifactId>
			<version>1.0-SNAPSHOT	</version>
		</dependency>

		<dependency>
			<groupId>com.snapdeal</groupId>
			<artifactId>SCOREClient</artifactId>
			<version>1.22-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.snapdeal</groupId>
			<artifactId>CocofsClient</artifactId>
			<version>1.11-SNAPSHOT</version>
		</dependency>
		<dependency>
